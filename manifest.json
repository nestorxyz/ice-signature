{
  "name": "Ice EIP-712 Signatures",
  "description": "Integrating EIP-712 for secure and reliable web3 transactions",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Ice EIP-712 Signatures"
  }
}
